<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pablolopezs.grepaut">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.SplashTheme"
        android:name=".data.dao.GreapautApplication">

        <activity android:name=".ui.login.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".ui.login.register.RegistroActivity">
        </activity>

        <activity
            android:name=".ui.MainActivity.MainActivity"
            android:label="@string/title_activity_manager"
            android:theme="@style/AppTheme.SplashTheme.NoActionBar">



        </activity>

    </application>

</manifest>